   ],
   "source": [
    "a, b, c, d, e, f, x1, x2, x3 = var('a b c d e f x1 x2 x3')\n",
    "S = matrix([[a, b, c], [b, d, e], [c, e, f]])\n",
    "x = vector([x1, x2, x3]);\n",
    "\n",
    "cw = x.dot_product(x)\n",
    "S* cw"
   ]
  },
